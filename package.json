{
  "name": "vercel-proxy-shield",
  "version": "1.0.0",
  "description": "Vercel Edge Function proxy with free proxy pool and fallback",
  "main": "api/index.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node server.js",
    "build:frontend": "vite build",
    "build:api": "tsc --project tsconfig.json --outDir dist",
    "build": "npm run build:frontend && npm run build:api",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@vercel/edge": "^1.1.1",
    "@vercel/kv": "^3.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "diff": "^8.0.3",
    "esbuild": "^0.24.2",
    "path-to-regexp": "^6.3.0",
    "pg": "^8.13.1",
    "undici": "^6.22.1",
    "vite": "^7.3.1",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "typescript": "^5.3.0",
    "vercel": "^32.3.0",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "diff": "^8.0.3",
    "path-to-regexp": "^6.3.0",
    "tar": "^7.5.3",
    "undici": "^6.22.1",
    "esbuild": {
      ".": "^0.24.2"
    }
  }
}
