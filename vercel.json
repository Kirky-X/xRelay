{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "nodeVersion": "20.x",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  }
}